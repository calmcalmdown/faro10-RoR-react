<p id="notice"><%= notice %></p>

<h1>Listing User Statuses</h1>

<table>
  <thead>
    <tr>

      <th>ID</th>
      <th>User ID</th>
      <th>Journal entry</th>
      <th>Anx attack</th>
      <th>Have headache</th>
      <th>Created At:</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_statuses.each do |user_status| %>
      <tr>

        <td><%= user_status.id %></td>
        <td><%= user_status.user_id %></td>
        <td><%= user_status.journal_entry %></td>
        <td><%= user_status.anx_attack %></td>
        <td><%= user_status.have_headache %></td>
        <td><%= user_status.created_at %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User status', new_user_status_path %>
