<table id= "patients" cellpadding="0" cellspacing="0" border="0" class="dataTable table table-striped table-hover table-highlight table-bordered">
  <thead>
  <tr>
    <th>Clients</th>
    <th>Status</th>
    <th>Avg Mood</th>
    <!--<th>Hospital</th>-->
    <th>Panic Attacks</th>
    <th>Suicidal Thoughts</th>
    <th>Reports</th>
    <th>Actions</th>
  </tr>
  </thead>
  <tbody>
  <% if @memberships.present? %>
      <% @memberships.each do |membership| %>
          <% if membership.inactive? %>
              <%= render "patients/row_inactive", membership: membership %>
          <% end %>
      <% end %>
  <% end %>
  </tbody>
</table>


