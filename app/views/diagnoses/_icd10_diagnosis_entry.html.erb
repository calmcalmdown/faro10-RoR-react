<h3>ICD-10</h3>

<%= form_for(@new_icd_diagnosis, url: "/diagnoses", method: :post) do |f| %>
  <div class="form-group">
  <!--<= f.hidden_field :medical_condition_id, value: @dsm5s.pluck(:icd10_code) %>-->
  <%= f.hidden_field :member_id, value: member_id %>
  <%= f.select :medical_condition_id,
    {},
    {},
    id: "icd10-select"
    %><br>
  </div>
  <%= f.submit "Add ICD Condition", class: "btn btn-info", style: "background-color: #326867; width: 170px;  border-radius: 0; border-color: white "  %>
<% end %>
