<p id="notice"><%= notice %></p>

<h1>Listing Activity Levels</h1>

<table>
  <thead>
    <tr>

      <th>Activity level</th>
      <th>Created At:</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @activity_levels.each do |activity_level| %>
      <tr>

        <td><%= activity_level.activity_level %></td>
        <td><%= activity_level.created_at %></td>


      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activity level', new_activity_level_path %>
